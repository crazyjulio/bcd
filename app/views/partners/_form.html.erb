<%= form_for partner do |f| %>
<div class="field">
  <%= f.label :name %>
  <br/>
  <%= f.text_field :name, placeholder: "Brothers Brick" %>
</div>
<div class="field">
  <%= f.label :url %>
  <br/>
  <%= f.text_field :url, placeholder: "www.brothers-brick.com" %>
</div>
<div class="field">
  <%= f.label :contact, "Contact Email" %>
  <br/>
  <%= f.text_field :contact, placeholder: "ralph@brothers-brick.com" %>
</div>
<div class="actions">
  <%= f.submit 'Save' %>
</div>
<% end %>